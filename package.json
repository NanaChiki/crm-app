{
  "name": "crm-app",
  "productName": "crm-app",
  "version": "1.0.0",
  "description": "小規模建築事業者向けCRMツール - React + Vite + Electron with HMR",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently --kill-others-on-fail \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "vite --port 5174",
    "dev:main": "cross-env NODE_ENV=development npm run build:main && npx electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "tsc --project tsconfig.main.json && tsc --project tsconfig.preload.json",
    "start": "electron .",
    "package": "electron-builder",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "keywords": [
    "electron",
    "react",
    "crm",
    "typescript"
  ],
  "author": {
    "name": "Daito",
    "email": "nanachiki256@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-date-pickers": "^8.10.2",
    "@prisma/client": "^6.14.0",
    "@types/papaparse": "^5.3.16",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "papaparse": "^5.5.3",
    "prisma": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/recharts": "^1.8.29",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^5.0.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "37.3.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  }
}
